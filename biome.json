{
	"$schema": "https://biomejs.dev/schemas/2.3.5/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"noNonNullAssertion": "off"
			},
			"suspicious": {
				"noEmptyBlock": "off",
				"noExplicitAny": "off",
				"noArrayIndexKey": "off"
			}
		}
	},
	"assist": {
		"enabled": false
	}
}
